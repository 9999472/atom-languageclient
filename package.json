{
  "name": "atom-languageclient",
  "version": "0.8.3",
  "description": "Integrate Language Servers with Atom",
  "repository": "https://github.com/atom/atom-languageclient",
  "license": "MIT",
  "main": "./build/lib/main",
  "types": "./build/lib/main.d.ts",
  "scripts": {
    "clean": "rm -rf build",
    "install": "npm run compile",
    "compile": "tsc",
    "watch": "tsc -watch",
    "lint": "tslint -c tslint.json 'lib/**/*.ts' 'test/**/*.ts'",
    "prepublish": "npm run clean && npm run compile",
    "test": "npm run compile && npm run lint && atom --test build/test"
  },
  "dependencies": {
    "@types/atom": "^1.23.2",
    "@types/node": "^8.0.41",
    "fuzzaldrin-plus": "^0.6.0",
    "vscode-jsonrpc": "^3.5.0",
    "vscode-languageserver-protocol": "^3.6.0-next.1",
    "vscode-languageserver-types": "^3.6.0-next.1"
  },
  "atomTestRunner": "./build/test/runner",
  "atomTranspilers": [
    {
      "glob": "{lib,test}/**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "cacheKeyFiles": [
          "package.json",
          ".babelrc"
        ]
      }
    }
  ],
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.1.3",
    "tslint": "^5.9.1",
    "atom-babel6-transpiler": "1.1.2",
    "atom-mocha-test-runner": "^1.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "chai": "^3.5.0",
    "eslint": "4.13.0",
    "eslint-config-fbjs-opensource": "^1.0.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "flow-bin": "^0.62.0",
    "mocha": "^3.2.0",
    "mocha-appveyor-reporter": "^0.4.0",
    "sinon": "^2.0.0",
    "typescript": "^2.6.1"
  }
}
